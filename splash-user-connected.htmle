<!-- Local script for menu handle -->
<!-- It can be also directive -->
<script>
    $(document).ready(function () {

        // Page scrolling feature
        $('a.page-scroll').bind('click', function(event) {
            var link = $(this);
            $('html, body').stop().animate({
                scrollTop: $(link.attr('href')).offset().top - 50
            }, 500);
            event.preventDefault();
        });

        $('body').scrollspy({
            target: '.navbar-fixed-top',
            offset: 80
        });

				$('li').highlight('Aura');
				$('p').highlight('Aura');
				$('span').highlight('Aura');
				$('a').highlight('Aura');
				$('h3').highlight('Aura');

				/**
         * Flot charts data and options
         */
        var data1 = [ [0, 5.5], [1, 4.8], [2, 4.0], [3, 3.6], [4, 4.0], [5, 6.0], [6, 5.0], [7, 5.1], [8,7.0] ];

        var chartUsersOptions = {
            series: {
                splines: {
                    show: true,
                    tension: 0.4,
                    lineWidth: 1,
                    fill: 0.4
                },
            },
            grid: {
                tickColor: "#f0f0f0",
                borderWidth: 1,
                borderColor: 'f0f0f0',
                color: '#6a6c6f'
            },
            colors: [ "#62cb31"],
						xaxis: {
							ticks: [[0,'-2:20'],[1,'-2 hr'],[2,'-1:40'],[3,'-1:20'],[4,'-1:10'],[5,'-1 hr'],[6,'-40 m'],[7,'-20 m'],[8,'now']]
						}
        };

        $.plot($("#flot-line-chart"), [data1], chartUsersOptions);

    });
</script>


